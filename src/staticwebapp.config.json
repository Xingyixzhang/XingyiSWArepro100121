{
    "routes": [
      {
        "route": "/",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/api",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/login",
        "rewrite": "/.auth/login/aad"
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      }
    ],
    "responseOverrides": {
      "401": {
        "redirect": "/login",
        "statusCode": 302
      }
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }
